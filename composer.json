{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
        "type": "package",
        "package": {
            "name": "wordpress/wordpress",
			"replace": {
				"johnpbloch/wordpress": "*"
			},
            "type": "wordpress-core",
            "version": "5.4",
            "source": {
                "type": "git",
                "url": "https://github.com/WordPress/WordPress.git",
                "reference": "5.4"
            }
        }
    }
  ],
  "require": {
    "wordpress/wordpress": "*",
    "wpackagist-plugin/addthis": "6.2.6",
    "wpackagist-plugin/akismet": "4.1.4",
    "wpackagist-plugin/broken-link-checker": "1.11.12",
    "wpackagist-plugin/cookie-notice": "1.2.51",
    "wpackagist-plugin/google-analytics-dashboard-for-wp": "6.0.2",
    "wpackagist-plugin/gtranslate": "2.8.54",
    "wpackagist-plugin/redirection": "4.7.1",
    "wpackagist-plugin/theme-my-login": "7.0.15",
    "wpackagist-plugin/w3-total-cache": "0.13.1",
    "wpackagist-plugin/wordpress-seo": "13.4",
    "wpackagist-plugin/wp-google-search": "1.0.8",
    "wpackagist-theme/twentyeleven": "3.4",
	"psycle-wordpress/wordpress-core-installer": "^1.0",
    "psycle-wordpress-plugins/mail-options": "^1.0"
  },
  "extra": {
    "wordpress-install-dir": "wp",
    "installer-paths": {
        "custom/mu-plugins/{$name}": ["type:wordpress-muplugin"],
        "custom/plugins/{$name}/": ["type:wordpress-plugin"],
        "custom/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
