{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
        "type": "package",
        "package": {
            "name": "wordpress/wordpress",
			"replace": {
				"johnpbloch/wordpress": "*"
			},
            "type": "wordpress-core",
            "version": "5.0.3",
            "source": {
                "type": "git",
                "url": "https://github.com/WordPress/WordPress.git",
                "reference": "5.0.3"
            }
        }
    }
  ],
  "require": {
    "wordpress/wordpress": "*",
    "wpackagist-plugin/addthis": "6.1.8",
    "wpackagist-plugin/akismet": "4.1",
    "wpackagist-plugin/broken-link-checker": "1.11.5",
    "wpackagist-plugin/cookie-notice": "1.2.46",
    "wpackagist-plugin/google-analytics-dashboard-for-wp": "5.3.7",
    "wpackagist-plugin/gutenberg": "4.9.0",
    "wpackagist-plugin/redirection": "3.7.2",
    "wpackagist-plugin/theme-my-login": "7.0.12",
    "wpackagist-plugin/visual-form-builder": "2.9.9",
    "wpackagist-plugin/w3-total-cache": "0.9.7.1",
    "wpackagist-plugin/wangguard": "1.7.3",
    "wpackagist-plugin/wordpress-seo": "9.5",
    "wpackagist-theme/twentyeleven": "3.1",
	"psycle-wordpress/wordpress-core-installer": "^1.0",
    "psycle-wordpress-plugins/mail-options": "^1.0"
  },
  "extra": {
    "wordpress-install-dir": "wp",
    "installer-paths": {
        "custom/mu-plugins/{$name}": ["type:wordpress-muplugin"],
        "custom/plugins/{$name}/": ["type:wordpress-plugin"],
        "custom/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
