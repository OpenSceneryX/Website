{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
        "type": "package",
        "package": {
            "name": "wordpress/wordpress",
			"replace": {
				"johnpbloch/wordpress": "*"
			},
            "type": "wordpress-core",
            "version": "5.4.2",
            "source": {
                "type": "git",
                "url": "https://github.com/WordPress/WordPress.git",
                "reference": "5.4.2"
            }
        }
    }
  ],
  "require": {
    "wordpress/wordpress": "*",
    "wpackagist-plugin/addthis": "6.2.6",
    "wpackagist-plugin/akismet": "4.1.7",
    "wpackagist-plugin/broken-link-checker": "1.11.14",
    "wpackagist-plugin/cookie-notice": "1.3.2",
    "wpackagist-plugin/google-analytics-dashboard-for-wp": "6.2.2",
    "wpackagist-plugin/gtranslate": "2.8.60",
    "wpackagist-plugin/redirection": "4.9.2",
    "wpackagist-plugin/responsive-menu": "3.1.30",
    "wpackagist-plugin/theme-my-login": "7.1.1",
    "wpackagist-plugin/w3-total-cache": "0.15.1",
    "wpackagist-plugin/wordpress-seo": "15.2",
    "wpackagist-plugin/wp-google-search": "1.0.9",
    "wpackagist-theme/twentyeleven": "3.5",
    "psycle-wordpress/wordpress-core-installer": "^1.0",
    "psycle-wordpress-plugins/mail-options": "^1.0"
  },
  "extra": {
    "wordpress-install-dir": "wp",
    "installer-paths": {
        "custom/mu-plugins/{$name}": ["type:wordpress-muplugin"],
        "custom/plugins/{$name}/": ["type:wordpress-plugin"],
        "custom/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
