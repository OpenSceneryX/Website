{
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "johnpbloch/wordpress": "4.2.3",
    "wpackagist-plugin/addthis": "5.0.12",
    "wpackagist-plugin/akismet": "3.1.3",
    "wpackagist-plugin/cookie-notice": "1.2.32",
    "wpackagist-plugin/google-analytics-dashboard-for-wp": "4.8.1.2",
    "wpackagist-plugin/move-wordpress-comments": "0.2.1.1",
    "wpackagist-plugin/redirection": "2.3.16",
    "wpackagist-plugin/theme-my-login": "6.3.12",
    "wpackagist-plugin/visual-form-builder": "2.8.3",
    "wpackagist-plugin/w3-total-cache": "0.9.4.1",
    "wpackagist-plugin/wangguard": "1.6.2",
    "wpackagist-plugin/wordpress-seo": "2.3.2"
  },
  "extra": {
    "wordpress-install-dir": "wp",
    "installer-paths": {
        "custom/mu-plugins/{$name}": ["type:wordpress-muplugin"],
        "custom/plugins/{$name}/": ["type:wordpress-plugin"],
        "custom/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
